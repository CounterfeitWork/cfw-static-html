{
  "name": "cfw-static-html",
  "private": true,
  "version": "1.0.0",
  "description": "Static HTML one-pager for CFW project",
  "repository": "https://github.com/julesrenaud/cfw-static-html",
  "author": "Jules Renaud",
  "config": {
    "ssh_prod": "cfw-infra-prod:~/www/counterfeit.work/public_html",
    "principal-static-html-boilerplate-version": "1.3.3",
    "principal-frontend-boilerplate-version": "3.5.1"
  },
  "engines": {
    "node": "12.18.1"
  },
  "scripts": {
    "build-dev": "webpack --config ./static/src/config/webpack.config.js --mode=development --appType=static",
    "build-prod": "webpack --config ./static/src/config/webpack.config.js --mode=production --appType=static",
    "start-watch": "webpack --watch --config ./static/src/config/webpack.config.js --mode=development --appType=static",
    "start-dev": "webpack-dev-server --config ./static/src/config/webpack.config.js --mode=development --appType=static --devServer=true",
    "zip": "zip ../$npm_package_name-$(date \"+%s\").zip -r ./dist index.html",
    "deploy-prod": "rsync -r static/dist $npm_package_config_ssh_dev/static; rsync -v --stats --progress index.html ${npm_package_config_ssh_dev}",
    "open-repo": "open $npm_package_repository_url",
    "test-chrome": "jest --config=./test/jest.config.js --browser=chromium --detectOpenHandles",
    "test-firefox": "jest --config=./test/jest.config.js --browser=firefox --detectOpenHandles",
    "test-chrome-ci": "jest --config=./test/jest.config.js --browser=chromium --detectOpenHandles --max-workers 2 --passWithNoTests",
    "test-firefox-ci": "jest --config=./test/jest.config.js --browser=firefox --detectOpenHandles --max-workers 2 --passWithNoTests"
  },
  "devDependencies": {
    "json-merger": "^1.1.1",
    "@prettier/plugin-pug": "^1.4.4",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "acorn": "^7.3.1",
    "breakpoint-sass": "^2.7.1",
    "casual-browserify": "^1.5.19-2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^29.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "event-source-polyfill": "^1.0.15",
    "file-loader": "^4.3.0",
    "focus-visible": "^5.1.0",
    "formdata-polyfill": "^3.0.20",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-inline-svg-plugin": "^2.2.6",
    "html-webpack-plugin": "^4.3.0",
    "html-webpack-reload-plugin": "^2.0.0",
    "ie11-custom-properties": "^4.1.0",
    "intersection-observer": "^0.11.0",
    "load-google-maps-api": "^1.3.3",
    "loading-attribute-polyfill": "^1.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "modernizr": "^3.11.3",
    "node-sass": "^4.14.1",
    "node-sass-json-importer": "^4.3.0",
    "object-fit-images": "^3.2.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "picturefill": "^3.0.3",
    "postcss-critical-css": "^3.0.6",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "puppeteer": "^3.3.0",
    "purgecss": "^2.3.0",
    "purgecss-from-html": "^2.2.0",
    "purgecss-webpack-plugin": "^2.3.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.2",
    "stickyfilljs": "^2.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^8.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-fix-style-only-entries": "^0.5.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-modernizr-loader": "^5.0.0",
    "whatwg-fetch": "^3.2.0",
    "worker-plugin": "^4.0.3"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not OperaMini all"
  ],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "proseWrap": "always",
    "printWidth": 100,
    "overrides": [
      {
        "files": "*.pug",
        "options": {
          "parser": "pug",
          "attributeSeparator": "as-needed"
        }
      },
      {
        "files": "*.php",
        "options": {
          "parser": "php",
          "phpVersion": "7.1",
          "tabWidth": 4,
          "braceStyle": "1tbs"
        }
      }
    ]
  },
  "dependencies": {
    "assert": "^2.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0"
  },
  "optionalDependencies": {
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest": "^25.2.3",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/puppeteer": "^3.0.1",
    "jest": "^25.5.4",
    "jest-puppeteer": "^4.4.0",
    "eslint-plugin-jest": "^23.18.0",
    "ts-jest": "^25.5.1"
  }
}
